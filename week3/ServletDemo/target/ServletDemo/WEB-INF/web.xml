<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>HelloWorldServlet</servlet-name>
		<servlet-class>com.revature.servlets.HelloWorldServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>EmployeeServlet</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NewEmployeeServlet</servlet-name>
		<servlet-class>com.revature.servlets.NewEmployeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloWorldServlet</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeServlet</servlet-name>
		<url-pattern>/employees</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>NewEmployeeServlet</servlet-name>
		<url-pattern>/new</url-pattern>
	</servlet-mapping>
</web-app>
